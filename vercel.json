{
    "version":2,
    "builds":[
        {
            "src":"myproject/wsgi.py",
            "use":"@vercel/python",
            "config" :{
                "maxLambdaSize":"15mb",
                "runtime":"python3.10"
            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config" :{
                "distDir":"15mb"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)"
        },{
            "src":"/(.*)"
        }
    ]
}